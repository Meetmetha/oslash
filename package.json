{
  "name": "nestjs",
  "version": "1.0.0",
  "description": "Base Project",
  "author": "Mitesh Metha",
  "private": false,
  "license": "MIT",
  "keywords": [
    "nestjs-boilerplate",
    "nestjs-objection",
    "nestjs-starter-template",
    "nestjs-template"
  ],
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.11"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:dist": "node dist/src/main.js",
    "test": "jest --config src/test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "^0.6.0",
    "@nestjs/core": "^7.5.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.1.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.5",
    "@squareboat/nest-events": "0.0.5",
    "bcrypt": "*",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cli-table3": "^0.6.0",
    "dotenv": "^8.6.0",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.2.0",
    "inquirer": "^7.3.3",
    "knex": "^0.21.12",
    "lodash": "^4.17.21",
    "meilisearch": "^0.22.3",
    "moment": "^2.29.1",
    "mongodb": "^2.2.33",
    "mongoose": "^5.10.1",
    "mysql2": "^2.2.5",
    "nestjs-meilisearch": "^2.1.1",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "query-string": "^6.13.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.6.18",
    "@tsed/mongoose": "^6.22.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.3",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.177",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.3",
    "@types/yargs": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^27.4.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "collectCoverage": true,
    "verbose": true
  }
}
